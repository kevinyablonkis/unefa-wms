extends ../templates/template

block header
    nav.nav
        div
            img(src="../assets/img/logo-unefa.png", alt="logo-unefa")
            p.logo UNEFA
        ul.nav-list
            li: a(href="/") Inicio

block main

    section.all-subgruop
        div.add-container
            form(method="post" action="/imprint/all-subgroup/download-all" autocomplete="off")
                h1 Datos a Insertar
                div.all-container
                    div.admin-bienes
                        div.add-admin
                            label(for="admin") Administrador
                            input(id="admin" name="admin" type="text" autofocus required)
                        div.bienes
                            label(for="admbiene") Adm. Bienes
                            input(id="admbiene" name="admbiene" type="text" required)
                    div.date-decano
                        div.add-decano
                            label(for="decano") Decano
                            input(id="decano" name="decano" type="text" required)
                        div.add-decano
                            label(for="date") Fecha
                            input(id="date" name="date" type="text" required)
                    input.btn-input(type="submit" value="Crear")
            div.decorate